{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "MQL",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#busID"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#types"
		},
		{
			"include": "#strings-double"
		},
		{
			"include": "#strings-single"
		}
	],
	"repository": {
		"types": {
			"patterns": [
				{
					"name": "entity.name.class.mql",
					"match": "\\b(attribute|name|type|originator|id)\\b"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.mql",
					"match": "\\b(from|to|select|where)\\b"
				}
			]
		},
		"operators": {
			"patterns": [
				{
					"name": "keyword.operator.new.mql",
					"match": "(print|[+]|[-]|([=]{2}))"
				}
			]
		},
		"strings-double": {
			"name": "string.quoted.double.mql",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.mql",
					"match": "\\\\."
				}
			]
		},
		"strings-single": {
			"name": "string.quoted.single.mql",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"name": "constant.character.escape.mql",
					"match": "\\\\."
				}
			]
		},
		"busID": {
			"patterns": [
				{
					"name": "constant.numeric.mql",
					"match": "\\b([0-9]*[0-9][.][0-9]*[0-9][.][0-9]*[0-9][.][0-9]*[0-9])\\b"
				}
			]
		}
	},
	"scopeName": "source.mql"
}